{
  "name": "tstestapp",
  "version": "1.0.0",
  "description": "test nodejs and ts app",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon server.js --exec babel-node --presets env",
